/*******************************
         Site Overrides
*******************************/

.tab {
  max-height: 530px;
  overflow-y: scroll;
  overflow-x: hidden;
}

.row  {
  > .olive {
    border-bottom: solid 1px rgba(255, 255, 255, 0.3) !important;
  }
}

.row:last-of-type .olive {
  border-bottom: 0 !important;
}
